<table cellspacing="5">
  <th><tr><td>listen</td></tr></th>
  <tbody>
    <% list.forEach(function(subitem) { %>
    <tr>
      <td>
        <h5><%= subitem.title %></h5>
        <p><%= subitem.teaser %></p>
        <audio controls="">
          <source src='<%= subitem.href %>'>
        </audio>"
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
<p>
  <em>Not the location you wanted? <a href='soundspotindex.html'>Try a different search.</a>
</p>